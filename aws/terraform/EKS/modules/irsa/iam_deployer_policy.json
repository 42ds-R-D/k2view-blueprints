{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "s3:CreateBucket",
        "Resource": "arn:aws:s3:::*"
      },
      {
        "Effect": "Allow",
        "Action": "s3:DeleteBucket",
        "Resource": "arn:aws:s3:::*-k2view-*"
      },
      {
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:GetObject",
          "s3:DeleteObject",
          "s3:ListBucket"
        ],
        "Resource": [
          "arn:aws:s3:::*-k2view-*/*",
          "arn:aws:s3:::*-k2view-*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "cassandra:Create",
          "cassandra:CreateMultiRegionResource",
          "cassandra:DropMultiRegionResource",
          "cassandra:Drop",
          "cassandra:AlterMultiRegionResource"
        ],
        "Resource": "arn:aws:cassandra:eu-central-1:329508970117:keyspace/*_k2view/"
      }
    ]
  }